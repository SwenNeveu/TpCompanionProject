<Window x:Class="TpCompanionProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TpCompanionProject"
        mc:Ignorable="d"
        Title="TpCompanion" Height="450" Width="800">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="109*"/>
            <ColumnDefinition Width="291*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Margin="10,10,151,10">
            <Button Content="TPs" Width="57" Height="138" Name="ButtonVoirTps"/>
            <Button Content="Promo" Width="57"  Height="138" Name="ButtonVoirPromo"/>
            <Button Content="Aide" Width="57"  Height="138" Name="ButtonVoirAide" Click="ButtonVoirAide_Click"/>

        </StackPanel>
        <Frame Grid.ColumnSpan="2" Margin="72,0,10,10" NavigationUIVisibility="Hidden" x:Name="FrameTpCompanion"/>


    </Grid>
</Window>
